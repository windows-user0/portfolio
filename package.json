{
    "name": "portfolio",
    "version": "0.2.0",
    "private": true,
    "dependencies": {
        "firebase": "^7.14.6",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.2.0",
        "react-scripts": "3.4.1"
    },
    "scripts": {
        "start": "yarn tailwind:css && react-scripts start",
        "tailwind:css": "tailwind build src/tailwind.css -c tailwind.config.js -o src/index.css",
        "build": "yarn tailwind:css && react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.3.2",
        "@testing-library/user-event": "^7.1.2",
        "@types/jest": "^25.2.3",
        "@types/node": "^14.0.2",
        "@types/react": "^16.9.35",
        "@types/react-dom": "^16.9.8",
        "@types/react-router": "^5.1.7",
        "@types/react-router-dom": "^5.1.5",
        "@types/react-typist": "^2.0.1",
        "tailwindcss": "^1.4.6",
        "typescript": "^3.9.2"
    }
}
